<% content_for (:head) do %>
      <%= stylesheet_link_tag "jquery.Jcrop.min" %>
      <%= javascript_include_tag "jquery.Jcrop.min" %>
<% end %>
<%= form_for @profile_picture do |f| %>
  <%= f.cropbox :asset, :width => 500 %>
  <%= f.crop_preview :asset, :width => 150 %>
  <%= f.submit 'Save' %>
<% end %>